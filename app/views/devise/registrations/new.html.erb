<div class:container>
<br><div class="col-md-1">
<h3 style="background-color:powderblue;">Sign Up</h3>
</div>
<div class="col-md-10">



<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="col-md-3">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-label" %>
  </div>

  <div class="col-md-3">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", class: "form-label" %>
  </div>

  <div class="col-md-3">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-label" %>
  </div>

   <div class="col-md-3">
    <%= f.label :employee_name  %><br />
    <%= f.text_field :user_name, class: "form-label" %>
  </div>

     <div class="col-md-3">
    <%= f.label :mobile_no   %><br />
    <%= f.text_field :user_mobileno , class: "form-label" %>
  </div>

    <div class="col-md-3">
    <%= f.label :date_of_birth %><br />
    <%= f.text_field :date_of_birth , class: "form-label" %>
  </div>

    <div class="field">
    <%= f.label :image  %><br />
    <%= f.file_field  :image, class: "form-label" %>
  </div>


  <div class="field">
    <%= f.label :skills  %><br />
    <%= f.text_field :skills, class: "form-label" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up", class:"btn btn-info btn-sm"  %>
  </div>
  </div>
<% end %>


<%= render "devise/shared/links" %>
</div>

